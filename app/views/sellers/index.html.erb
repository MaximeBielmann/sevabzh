<div class="container">
    <div class="row">
        <% @sellers.each do |seller| %>
                <div class="col-md-4 col-sm-6 my-auto text-center">
                <a href="/sellers/<%= seller.id %>"><%= image_tag (seller.logo_url), size: '200', :class => "img-fluid" %></a>
                <a href="/sellers/<%= seller.id %>"><h3><%= seller.vendor %></h3></a>
                </div>
        <% end %> 
    </div>
</div>