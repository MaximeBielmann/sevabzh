				<ul>
							<li class="p-b-9">
							  <%= link_to "Home", '/' %>
							</li>

							<li class="p-b-9">
								<%= link_to "Sneakers", '/sneakers' %>
							</li>

							<li class="p-b-9">
								<%= link_to "Contact", '/contact' %>
							</li>
				</ul>
			</div>

			<div class="w-size7 p-t-30 p-l-15 p-r-15 respon4">
				<h4 class="s-text12 p-b-30">
					Shops
				</h4>

				<ul>
							<li class="p-b-9">
							  <%= link_to "43einhalb", 'https://www.43einhalb.com/' %>
							</li>

							<li class="p-b-9">
								<%= link_to "asphaltgold.de", 'https://asphaltgold.de/de/' %>
							</li>

							<li class="p-b-9">
								<%= link_to "Allike", 'https://www.allikestore.com/' %>
							</li>
				</ul>
			</div>

			<div class="w-size7 p-t-30 p-l-15 p-r-15 respon4">
				<h4 class="s-text12 p-b-30">
					Help
				</h4>

				<ul>
							<li class="p-b-9">
							  <%= link_to "Report a bug", '/contact' %>
							</li>

							<li class="p-b-9">
								<%= link_to "Contact", '/contact' %>
							</li>

							<li class="p-b-9">
								<%= link_to "FAQs", '/faq' %>
							</li>

							<li class="p-b-9">
								<%= link_to "Partenariat", '/contact' %>
							</li>
				</ul>
			</div>

			<div class="w-size8 p-t-30 p-l-15 p-r-15 respon3">
				<h4 class="s-text12 p-b-30">
					Newsletter
				</h4>

				<!-- Begin Mailchimp Signup Form -->
