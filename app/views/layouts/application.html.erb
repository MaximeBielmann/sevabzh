  </head>
    
    <!-- Header -->
	<header class="header1">
		<!-- Header desktop -->
		<div class="container-menu-header">
			<div class="topbar">
				<div class="topbar-social">
					<a href="https://www.facebook.com/seva.bzh/" target="_blank" class="topbar-social-item fab fa-facebook"></a>
					<a href="https://www.instagram.com/seva.bzh/" target="_blank" class="topbar-social-item fab fa-instagram"></a>
					<a href="https://twitter.com/seva_bzh" target="_blank" class="topbar-social-item fab fa-twitter"></a>
					<a href="https://www.youtube.com/channel/UCnmFFggxadFfYhLNNOhXCPw" target="_blank" class="topbar-social-item fab fa-youtube"></a>
				</div>

				<span class="topbar-child1">
					Économisez jusqu'à 50% sur l'achat de vos sneakers
				</span>

				<div class="topbar-child2">
					<span class="topbar-email">
						contact@seva.bzh
					</span>
				</div>
			</div>

			<div class="wrap_header">
				<!-- Logo -->
				<a href="/" class="logo">
					<%= image_tag "logo.png" %>
				</a>

				<!-- Menu -->
				
				<div class="wrap_menu">
					<nav class="menu">
						<ul class="main_menu">
							<li>
							  <%= link_to "Home", '/' %>
							</li>
							
							<li>
								<%= link_to "Sneakers", '/sneakers' %>
							</li>
							
							<li>
							  <%= link_to "Boutiques", '/sellers' %>
							</li>
							
							<li>
							  <%= link_to "Calendrier", '/calendar' %>
							</li>

							<li>
								<%= link_to "Contact", '/contact' %>
							</li>
						</ul>
					</nav>
				</div>
			</div>
		</div>
		
		<!-- DEBUT DU TEST -->
		
			<!-- Header Mobile -->
		<div class="wrap_header_mobile">
			<!-- Logo moblie -->
			<a href="/" class="logo-mobile">
				<%= image_tag "logo.png" %>
			</a>
			<div class="wrap_menu">
					<nav class="menu">
						<ul class="main_menu">
							<li>
							  <%= link_to "Home", '/' %>
							</li>

							<li>
								<%= link_to "Sneakers", '/sneakers' %>
							</li>
							
							<li>
							  <%= link_to "Boutiques", '/sellers' %>
							</li>

							<li>
								<%= link_to "Contact", '/contact' %>
							</li>
						</ul>
					</nav>
				</div>
		</div>

		<!-- Global site tag (gtag.js) - Google Analytics -->
			<script async src="https://www.googletagmanager.com/gtag/js?id=UA-31618814-5"></script>
			<script>
			  window.dataLayer = window.dataLayer || [];
			  function gtag(){dataLayer.push(arguments);}
			  gtag('js', new Date());
			
			  gtag('config', 'UA-31618814-5');
			</script>
							
		  </header>
  
  <body>
  	 <!-- Load Facebook SDK for JavaScript -->
		<div id="fb-root"></div>
		<script>(function(d, s, id) {
		  var js, fjs = d.getElementsByTagName(s)[0];
		  if (d.getElementById(id)) return;
		  js = d.createElement(s); js.id = id;
		  js.src = 'https://connect.facebook.net/fr_FR/sdk/xfbml.customerchat.js#xfbml=1&version=v2.12&autoLogAppEvents=1';
		  fjs.parentNode.insertBefore(js, fjs);
		}(document, 'script', 'facebook-jssdk'));</script>
		
		<!-- Your customer chat code -->
		<div class="fb-customerchat"
		  attribution=setup_tool
		  page_id="983389791795280"
		  theme_color="#f3d1bb"
		  logged_in_greeting="Salut, vous voulez une super info ?"
		  logged_out_greeting="Salut, vous voulez une super info ?">
		</div>
		
  	<div><%= flash[:info] %></div>
    <%= yield %>
  </body>
  
